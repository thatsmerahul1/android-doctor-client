<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- navigation item -->
    <declare-styleable name="NavigationItem">
        <attr name="niIndicator" format="reference" />
        <attr name="niLabel" format="reference" />
        <attr name="niTitle" format="reference" />
        <attr name="niIcon" format="reference" />
        <attr name="niIconImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>


</resources>